{
  "manifest_version": 2,

  "name": "Workout share in GC",
  "description": "Share workouts between users in Garmin Connect",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Garmin Share Workout!"
  },

    "permissions": [
      "activeTab",
      "https://ajax.googleapis.com/",
      "tabs",
      "notifications",
      "http://*/",
      "https://*/"
    ]
  }


